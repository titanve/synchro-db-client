{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "classes": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "extends": ["prettier", "prettier/react", "plugin:prettier/recommended"],
  "plugins": ["react", "jsx-a11y", "import"]
}
